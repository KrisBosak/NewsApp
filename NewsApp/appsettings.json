{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "[FutureVariable]"
  },
  "Jwt": {
    "Key": "[FutureJwtVariable]",
    "Issuer": "https://localhost",
    "Audience": "https://localhost",
    "ExpirationInMinutes": 60,
    "AuthorizationClaim": "role"
  },
  "AllowedHosts": "*"
}
